{
  "name": "Night City",
  "type": "dark",
  "colors": {
    
    // === Editor - Tief im Neon-Dunst ===
    "editor.background": "#0f1419",
    "editor.foreground": "#e0e0e8",
    "editorLineNumber.foreground": "#3a4a5f",
    "editorLineNumber.activeForeground": "#4a9eff",
    "editorCursor.foreground": "#5fcfe4",
    "editor.selectionBackground": "#4a9eff33",
    "editor.inactiveSelectionBackground": "#2a3a4f22",
    "editor.lineHighlightBackground": "#141a2288",
    "editor.selectionHighlightBackground": "#4a9eff20",
    "editor.wordHighlightBackground": "#4dd0e120",
    "editor.wordHighlightStrongBackground": "#4dd0e130",
    "editor.findMatchBackground": "#ba5f9f44",
    "editor.findMatchHighlightBackground": "#ba5f9f22",
    "editorBracketMatch.background": "#4a9eff22",
    
    // === Sidebar - Straßen von Night City ===
    "sideBar.background": "#0f1419",
    "sideBar.foreground": "#b8b8c8",
    "sideBar.border": "#00000000",
    "sideBarTitle.foreground": "#e0e0e8",
    "sideBarSectionHeader.background": "#0f1419",
    "sideBarSectionHeader.foreground": "#4a9eff",
    "sideBarSectionHeader.border": "#00000000",
    
    // === Activity Bar - Neonreklamen ===
    "activityBar.background": "#0f1419",
    "activityBar.foreground": "#4a9eff",
    "activityBar.inactiveForeground": "#5a6a7a",
    "activityBar.border": "#00000000",
    "activityBarBadge.background": "#ba5f9f",
    "activityBarBadge.foreground": "#ffffff",
    "activityBar.activeBorder": "#00000000",
    
    // === Status Bar - Cyberdeck Status ===
    "statusBar.background": "#2a5a8f",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#ba5f9f",
    "statusBar.debuggingBackground": "#c084fc",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBarItem.hoverBackground": "#1f4a7a",
    
    // === Tabs - Offene Datenströme ===
    "editorGroupHeader.tabsBackground": "#0f1419",
    "tab.activeBackground": "#0f1419",
    "tab.activeForeground": "#e0e0e8",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.inactiveBackground": "#0f1419",
    "tab.inactiveForeground": "#6a7a8a",
    "tab.border": "#00000000",
    "tab.hoverBackground": "#141a2255",
    
    // === Terminal - Konsole ===
    "terminal.background": "#0f1419",
    "terminal.foreground": "#e0e0e8",
    "terminal.ansiBlack": "#14141f",
    "terminal.ansiRed": "#f87171",
    "terminal.ansiGreen": "#4ade80",
    "terminal.ansiYellow": "#fbbf24",
    "terminal.ansiBlue": "#60a5fa",
    "terminal.ansiMagenta": "#c084fc",
    "terminal.ansiCyan": "#5fcfe4",
    "terminal.ansiWhite": "#e0e0e8",
    "terminal.ansiBrightBlack": "#52525b",
    "terminal.ansiBrightRed": "#fca5a5",
    "terminal.ansiBrightGreen": "#86efac",
    "terminal.ansiBrightYellow": "#fcd34d",
    "terminal.ansiBrightBlue": "#93c5fd",
    "terminal.ansiBrightMagenta": "#d8b4fe",
    "terminal.ansiBrightCyan": "#a5f3fc",
    "terminal.ansiBrightWhite": "#f4f4f5",
    
    // === Buttons & Inputs ===
    "button.background": "#2a5a8f",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#3a6a9f",
    "input.background": "#0a0f14",
    "input.foreground": "#e0e0e8",
    "input.border": "#00000000",
    "input.placeholderForeground": "#5a6a7a",
    "inputOption.activeBorder": "#00000000",
    "inputOption.activeBackground": "#4a9eff22",
    
    // === Listen & Dropdowns ===
    "list.activeSelectionBackground": "#2a5a8f44",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#2a3a4f33",
    "list.hoverBackground": "#1f2a35",
    "list.focusBackground": "#2a5a8f33",
    "list.highlightForeground": "#5fcfe4",
    
    // === Panel (Terminal, Output, Debug) ===
    "panel.background": "#0f1419",
    "panel.border": "#00000000",
    "panelTitle.activeBorder": "#00000000",
    "panelTitle.activeForeground": "#e0e0e8",
    "panelTitle.inactiveForeground": "#5a6a7a",
    
    // === Scrollbar ===
    "scrollbarSlider.background": "#2a3a4f44",
    "scrollbarSlider.hoverBackground": "#2a3a4f66",
    "scrollbarSlider.activeBackground": "#2a3a4f88",
    
    // === Badges & Notifications ===
    "badge.background": "#ba5f9f",
    "badge.foreground": "#ffffff",
    "notificationCenter.border": "#00000000",
    "notifications.background": "#0a0f14",
    "notifications.border": "#00000000",
    
    // === Git Decoration ===
    "gitDecoration.modifiedResourceForeground": "#fbbf24",
    "gitDecoration.deletedResourceForeground": "#f87171",
    "gitDecoration.untrackedResourceForeground": "#4ade80",
    "gitDecoration.ignoredResourceForeground": "#52525b",
    "gitDecoration.conflictingResourceForeground": "#fb923c",
    
    // === GLOW EFFECTS - Neon Suchfenster ===
    "focusBorder": "#00000000",
    "editorWidget.background": "#0a0f14",
    "editorWidget.border": "#7ea3d8cc",
    "editorWidget.foreground": "#e0e0e8",
    "editorWidget.resizeBorder": "#7ea3d8",
    "editorSuggestWidget.background": "#0a0f14",
    "editorSuggestWidget.border": "#9ab8d866",
    "editorSuggestWidget.foreground": "#e0e0e8",
    "editorSuggestWidget.selectedBackground": "#4a9eff22",
    "editorHoverWidget.background": "#0a0f14",
    "editorHoverWidget.border": "#89cfe866",
    "editorHoverWidget.foreground": "#e0e0e8",
    "editorHoverWidget.statusBarBackground": "#0a0f14",
    
    // ===  PEEK VIEW - Seamless ===
    "peekView.border": "#00000000",
    "peekViewEditor.background": "#0f1419",
    "peekViewResult.background": "#0a0f14",
    "peekViewTitle.background": "#0a0f14",
    "peekViewResult.selectionBackground": "#4a9eff22",
    
    // ===  ERROR/WARNING - Clean ===
    "editorError.foreground": "#ff5d87",
    "editorError.border": "#00000000",
    "editorWarning.foreground": "#ffaa5a",
    "editorWarning.border": "#00000000",
    "editorInfo.foreground": "#5fcfe4",
    "editorInfo.border": "#00000000",
    "editorHint.foreground": "#c173e0",
    "editorHint.border": "#00000000",
    
    // === SEARCH NEON EFFECTS - Suchfenster Highlights ===
    "tab.activeModifiedBorder": "#00000000",
    "editor.findMatchBorder": "#cf8fbe",
    "editor.findMatchHighlightBorder": "#cf8fbe44",
    "editor.rangeHighlightBackground": "#4a9eff11",
    "editor.rangeHighlightBorder": "#00000000",
    "editorBracketMatch.border": "#00000000",
    
    // ===  BRACKET PAIR COLORIZATION - Soft Pastels ===
    "editorBracketHighlight.foreground1": "#7ea3d8",
    "editorBracketHighlight.foreground2": "#b88ac9",
    "editorBracketHighlight.foreground3": "#cf8fbe",
    "editorBracketHighlight.foreground4": "#9ab8d8",
    "editorBracketHighlight.foreground5": "#89cfe8",
    "editorBracketHighlight.foreground6": "#aa95d9",
    
    // ===  GIT GUTTER - Clean ===
    "editorGutter.addedBackground": "#7bb3a044",
    "editorGutter.modifiedBackground": "#9ab8d844",
    "editorGutter.deletedBackground": "#cf8fbe44",
    
    // ===  DIFF EDITOR - Clean ===
    "diffEditor.insertedTextBackground": "#7bb3a022",
    "diffEditor.removedTextBackground": "#cf8fbe22",
    "diffEditor.insertedLineBackground": "#7bb3a011",
    "diffEditor.removedLineBackground": "#cf8fbe11",
    "diffEditor.border": "#00000000",
    
    // ===  SEARCH HIGHLIGHT - Clean ===
    "search.resultsInfoForeground": "#9ab8d8",
    "searchEditor.findMatchBackground": "#9ab8d833",
    "searchEditor.findMatchBorder": "#00000000",
    
    // ===  NOTIFICATION CENTER - Clean ===
    "notificationCenterHeader.background": "#0a0f14",
    "notificationCenterHeader.foreground": "#9ab8d8",
    "notifications.foreground": "#e0e0e8",
    "notificationLink.foreground": "#89cfe8",
    "notificationToast.border": "#00000000",
    
    // ===  CLEAN & ZEN - Keine Guides/Rulers ===
    "progressBar.background": "#7ea3d8",
    "editorCursor.background": "#0f1419",
    "editorLink.activeForeground": "#89cfe8",
    "selection.background": "#9ab8d833",
    "editorWhitespace.foreground": "#00000000",
    "editorIndentGuide.background": "#00000000",
    "editorIndentGuide.activeBackground": "#00000000",
    "editorRuler.foreground": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Kommentare - Soft Gray",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a7a8a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings - Soft Cyan",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#89cfe8"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#9acfe8"
      }
    },
    {
      "name": "Zahlen - Soft Blue",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "Boolean & None/Null",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.none"
      ],
      "settings": {
        "foreground": "#b88ac9"
      }
    },
    {
      "name": "Keywords - Soft Purple",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#aa95d9"
      }
    },
    {
      "name": "Storage Keywords (class, def, async, etc.)",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.declaration"
      ],
      "settings": {
        "foreground": "#aa95d9"
      }
    },
    {
      "name": "Import/From Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#aa95d9"
      }
    },
    {
      "name": "Operatoren",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#9aacbe"
      }
    },
    {
      "name": "Funktionen - Soft Purple Pink",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#cf8fbe"
      }
    },
    {
      "name": "Function Calls",
      "scope": [
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#d89fcc"
      }
    },
    {
      "name": "Klassen - Soft Blue",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#7ea3d8"
      }
    },
    {
      "name": "Vererbte Klassen",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#8eb3e8"
      }
    },
    {
      "name": "Dekoratoren - Soft Pink",
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "punctuation.definition.decorator"
      ],
      "settings": {
        "foreground": "#cf8fbe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variablen - Soft Gray",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#b8b8c8"
      }
    },
    {
      "name": "Parameter - Light Gray",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#a8b8c8"
      }
    },
    {
      "name": "self, this, cls",
      "scope": [
        "variable.language.special.self",
        "variable.language.this",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#b88ac9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Eigenschaften & Attribute",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "Konstanten",
      "scope": [
        "constant",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "Module & Namespaces",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#7ea3d8"
      }
    },
    {
      "name": "Built-in Functions/Types",
      "scope": [
        "support.function.builtin",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#aa95d9"
      }
    },
    {
      "name": "Tag Attribute Names",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#7ea3d8"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#89cfe8"
      }
    },
    {
      "name": "Markdown Überschriften",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#aa95d9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#7ea3d8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#cf8fbe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#89cfe8"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#9ab8d8"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#b88ac9"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#aa95d9"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#8a9ab8"
      }
    },
    {
      "name": "Invalid/Fehler",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff5d87",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffaa5a",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
